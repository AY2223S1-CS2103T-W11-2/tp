<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="slotPane" fx:id="slotPane" alignment="BASELINE_LEFT" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="TOP_LEFT" prefHeight="70.0" prefWidth="95.0" spacing="2.0" style="-fx-background-color: #8ac6d1#8ac6d1; -fx-background-radius: 10px;" GridPane.columnIndex="0">
        <padding>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
        </padding>
        <HBox alignment="CENTER_LEFT" spacing="2.0">
            <Label fx:id="module" text="\$module" />
        </HBox>
        <Label fx:id="time" text="\$time" />
        <Label fx:id="type" text="\$type" />
        <Label fx:id="venueName" text="\$venueName" />
      <HBox.margin>
         <Insets />
      </HBox.margin>
    </VBox>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
